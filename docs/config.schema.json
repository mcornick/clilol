{
  "$id": "https://mcornick.com/clilol/config.schema.json",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "additionalProperties": false,
  "description": "clilol configuration file",
  "properties": {
    "address": {
      "default": "",
      "description": "address on your omg.lol account",
      "type": "string"
    },
    "apikey": {
      "default": "",
      "description": "API key for your omg.lol account",
      "type": "string"
    },
    "email": {
      "default": "",
      "description": "email used to log into your omg.lol account",
      "type": "string"
    }
  },
  "title": "clilol config",
  "type": "object"
}
